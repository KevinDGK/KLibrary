<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.dgk.demo.DemoActivity">

    <Button
        android:id="@+id/btn_test_demo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAllCaps="false"
        android:text="Demo"/>

    <Button
        android:id="@+id/btn_test_exception"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAllCaps="false"
        android:text="异常"/>

    <Button
        android:id="@+id/btn_test_concat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAllCaps="false"
        android:text="concat"/>


    <Button
        android:id="@+id/btn_test_zip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAllCaps="false"
        android:text="zip"/>

    <Button
        android:id="@+id/btn_test_map"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAllCaps="false"
        android:text="map"/>

    <Button
        android:id="@+id/btn_test_flatmap"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAllCaps="false"
        android:text="flatMap"/>

    <Button
        android:id="@+id/btn_test_concatmap"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAllCaps="false"
        android:text="concatMap"/>

    <Button
        android:id="@+id/btn_test_interval"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAllCaps="false"
        android:text="interval"/>

</LinearLayout>
